{
  "presets": ["react", "es2015", "stage-0"],
  "plugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-react-display-name",
    ["system-import-transformer", {"modules": "common"}]
  ],
  "env": {
    "development": {
      "plugins": [
        "typecheck"
      ]
    },
    "production": {
      "plugins": [
        "transform-react-inline-elements",
        "transform-react-constant-elements"
      ]
    }
  }
}
